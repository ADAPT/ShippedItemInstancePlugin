      <dependency id="System.ComponentModel.DataAnnotations" version="5.0.0" />
    </dependencies>
    <references>
      <reference file="AgGateway.ADAPT.ShippedItemInstancePlugin.dll"/>
    </references>
  </metadata>
  <files>
    <file src="ShippedItemInstancePlugin/bin/Release/net8.0/AgGateway.ADAPT.ShippedItemInstancePlugin.dll" target="lib/netstandard2.0/AgGateway.ADAPT.ShippedItemInstancePlugin.dll"/>
